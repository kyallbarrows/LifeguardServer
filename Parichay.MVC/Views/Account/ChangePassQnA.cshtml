@model Parichay.MVC.Models.ChangePassQnA

@{
    ViewBag.Title = "ChangePassQnA";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Change Your Password Recovery Question & Answer</h2>

<script src="@Url.Content("~/Content/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Change Your Password Recovery Question & Answer</legend>
        <table class="tblAllBorders">
        <tr><th><div class="editor-label">
            @Html.LabelFor(model => model.PasswordQuestion)
        </div>
        </th><td><div class="editor-field">
            @Html.EditorFor(model => model.PasswordQuestion)
            @Html.ValidationMessageFor(model => model.PasswordQuestion)
        </div></td></tr>

        <tr><th><div class="editor-label">
            @Html.LabelFor(model => model.PasswordAnswer)
        </div>
        </th><td><div class="editor-field">
            @Html.EditorFor(model => model.PasswordAnswer)
            @Html.ValidationMessageFor(model => model.PasswordAnswer)
        </div></td></tr>

        <tr><th><div class="editor-label">
            @Html.LabelFor(model => model.ConfirmPasswordAnswer)
        </div>
        </th><td><div class="editor-field">
            @Html.EditorFor(model => model.ConfirmPasswordAnswer)
            @Html.ValidationMessageFor(model => model.ConfirmPasswordAnswer)
        </div></td></tr>

        <tr><th><div class="editor-label">
            @Html.LabelFor(model => model.AccPassword)
        </div>
        </th><td><div class="editor-field">
            @Html.EditorFor(model => model.AccPassword)
            @Html.ValidationMessageFor(model => model.AccPassword)
        </div></td></tr>
        <tr><td colspan="2"><input type="submit" value="Change" /></td></tr>

        </table>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to Home", "Index", "Account")
</div>
