@model Parichay.Data.Entity.MemberFriends

@{
    ViewBag.Title = "Create";
    System.Collections.IList srchResult = ViewBag.searchResults;
}



@section Styles {

    <link href="@Url.Content("~/Content/styles/jquery.ui.css")" rel="stylesheet" type="text/css" />
    }
<script src="@Url.Content("~/Content/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
<fieldset>
<legend><h2>Find a Friend</h2></legend>
@using (Html.BeginForm("Find","Friend"))
{
    <table class="tblAllBorders" width="400px">
    <tr><th>Find By Name:</th></tr>
<tr><td>
<input type="text" id="givenEmail" name="givenEmail" />
</td></tr>
<tr>
<td><input type="submit" value="Find!" /></td>
</tr>
<tr>
<td><div id="outpDiv">
@if (srchResult != null)
{
    @Html.Partial("UsrSrch", srchResult);
}
</div></td>
</tr>
</table>
}

<br /><br />
</fieldset>
<div>
    @Html.ActionLink("Back to MyFriends", "Index")
</div>
